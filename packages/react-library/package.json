{
	"name": "@nrcs/react-library",
	"description": "",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"scripts": {
		"build": "node ../../scripts/build.js",
		"test": "node ../../scripts/test.js",
		"storybook": "node ../../scripts/checkname.js&&start-storybook -p 6006 --no-dll",
		"build-storybook": "node ../../scripts/checkname.js&&build-storybook --no-dll"
	},
	"dependencies": {
		"mobx": "^4.15.7",
		"mobx-react": "^6.3.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"reactstrap": "^8.4.1",
		"regenerator-runtime": "^0.13.7"
	},
	"devDependencies": {
		"@types/reactstrap": "^8.4.1",
		"typescript": "^4.0.3"
	},
	"browserslist": {
		"production": [
			"ie 11"
		],
		"development": [
			"ie 11"
		]
	},
	"jest": {
		"setupFilesAfterEnv": [
			"./setupTests.js"
		],
		"moduleNameMapper": {
			"^@[/](.+)": "<rootDir>/src/$1"
		},
		"automock": false,
		"reporters": [
			"default",
			"jest-junit"
		]
	},
	"jest-junit": {
		"suiteName": "jest tests",
		"outputName": "junit-test-report.xml"
	}
}
